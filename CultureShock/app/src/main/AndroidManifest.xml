<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.angelhacks.cultureshock" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".signup_e" >
        </activity>
        <activity
            android:name=".DestinationList"
            android:label="@string/title_activity_destination_list"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.angelhacks.cultureshock.MainActivity" />
        </activity>
        <activity
            android:name=".Categories"
            android:label="@string/title_activity_categories"
            android:parentActivityName=".DestinationList" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.angelhacks.cultureshock.DestinationList" />
        </activity>

        <activity android:name="com.angelhacks.cultureshock.signup_f" >
        </activity>


    </application>

</manifest>
